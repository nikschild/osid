BEGIN TRANSACTION;
CREATE TABLE "incidents" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "name" varchar(255), "firstseen" date, "user_id" integer, "summary" text, "pvname" varchar(255), "pvsector_id" integer, "svname" varchar(255), "svsector_id" integer, "attackerdescription" varchar(255), "attacker_id" integer, "attackdescription" varchar(255), "attack_id" integer, "targetdescription" varchar(255), "target_id" integer, "country_id" integer, "created_at" datetime, "updated_at" datetime, "reviewed" boolean, "weaknessdescription" varchar(255), "weakness_id" integer);
